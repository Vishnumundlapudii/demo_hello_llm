# E2E Networks LLM Configuration
# Copy this file to .env and fill in your actual values

# E2E Networks LLM Endpoint URL
# Example: https://your-llm-endpoint.e2enetworks.com/api/v1/chat/completions
E2E_ENDPOINT_URL=your_e2e_endpoint_url_here

# API Key for E2E Networks LLM
# Get this from your E2E Networks dashboard
E2E_API_KEY=your_api_key_here

# Optional: Model name (if your endpoint supports multiple models)
E2E_MODEL_NAME=e2e-llm

# Optional: Default model parameters
DEFAULT_TEMPERATURE=0.7
DEFAULT_MAX_TOKENS=1000
DEFAULT_TOP_P=0.9

# Optional: Request timeout (in seconds)
REQUEST_TIMEOUT=30